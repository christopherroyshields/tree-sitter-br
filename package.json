{
  "name": "tree-sitter-br",
  "version": "0.25.1",
  "description": "Tree-sitter parser for Business Rules! language.",
  "main": "index.js",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "br"
  ],
  "author": "Christopher Shields",
  "license": "ISC",
  "files": [
    "build/Release/tree_sitter_br_binding.node",
    "index.js",
    "src/node-types.json"
  ],
  "devDependencies": {
    "electron-rebuild": "^3.2.9",
    "gulp": "^5.0.0",
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2",
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.25.6",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "parse": "tree-sitter parse example.brs",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "tree-sitter test",
    "build": "tree-sitter generate && tree-sitter build",
    "build:electron": "npx electron-rebuild -v 34.3.2 -f -w .",
    "build:wasm": "tree-sitter build --wasm"
  }
}
